input FormCreateOrder {
    productIds: [String]
    deliveryAddress: String
    paymentType: String
}

type Order {
    id: String
    totalMoney: String
    quantity: Int
    discount: Int
    createdAt: String
    createdBy: String
    orderDetails: [OrderDetail]
}

type OrderDetail {
    id: String
    product: Product
}

type OrderListPaginate {
    data: [Order]
    pagination: PaginationData
}